<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="300dp"
        android:background="@color/white"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:padding="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/dialog_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/cabin_sketch"
            android:text="Data Pasien"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/tvNomorBPJS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/gabriela"

            android:text="No. BPJS Pasien : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dialog_title" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Nama Pasien : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_nomor" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Umur Pasien : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_nama" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Jenis Kelamin : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_umur" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:text="Golongan Darah : "
            android:layout_marginTop="14dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_gender" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Alamat : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_goldar" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Nomor Telepon : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_alamat" />
        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginTop="14dp"
            android:text="Keterangan : "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dialog_notelp" />

        <androidx.constraintlayout.widget.Barrier
            android:id="@+id/barrier2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            app:barrierDirection="end"
            app:constraint_referenced_ids="textView6,textView5,textView4,textView3,textView2,textView,tvNomorBPJS"
            tools:layout_editor_absoluteX="116dp"
            tools:layout_editor_absoluteY="14dp" />
        <TextView
            android:id="@+id/tv_dialog_nomor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="isi nomor BPJS pasien"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/tvNomorBPJS" />
        <TextView
            android:id="@+id/tv_dialog_nama"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="isi nama pasien"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView" />
        <TextView
            android:id="@+id/tv_dialog_umur"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="isi umur pasien"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView2" />
        <TextView
            android:id="@+id/tv_dialog_gender"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            android:text="jenis kelamin pasien"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView3"
            />
        <TextView
            android:id="@+id/tv_dialog_goldar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            android:text="isi goldar pasien"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView4" />

        <TextView
            android:id="@+id/tv_dialog_alamat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/gabriela"
            android:text="isi alamat pasien "

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView5" />

        <TextView
            android:id="@+id/tv_dialog_notelp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            android:text="isi telepon pasien"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView6" />

        <TextView
            android:id="@+id/tv_dialog_desc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gabriela"
            android:layout_marginStart="20dp"
            android:text="isi keterangan pasien"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/barrier2"
            app:layout_constraintTop_toTopOf="@+id/textView7" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>